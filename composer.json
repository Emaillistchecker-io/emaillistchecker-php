{
    "name": "emaillistchecker/emaillistchecker-php",
    "description": "Official PHP SDK for EmailListChecker email verification API",
    "keywords": [
        "email",
        "verification",
        "validation",
        "checker",
        "email-validation",
        "email-verification",
        "api",
        "sdk",
        "emaillistchecker"
    ],
    "homepage": "https://emaillistchecker.io",
    "license": "MIT",
    "authors": [
        {
            "name": "EmailListChecker",
            "email": "developers@emaillistchecker.io",
            "homepage": "https://emaillistchecker.io"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "EmailListChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmailListChecker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "support": {
        "email": "support@emaillistchecker.io",
        "issues": "https://github.com/Emaillistchecker-io/emaillistchecker-php/issues",
        "docs": "https://platform.emaillistchecker.io/api"
    }
}
